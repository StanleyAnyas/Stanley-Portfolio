const ZoomMtg = require('zoomus-jssdk');

const meetingId = '9114704692'
const apiKey = 'Eak2h6zKQWKL2DC2tY3xJQ'
const apiSecret = 'SAUuuzTgN6pTSxZizPLvsawi94MmV0Up5p84'

const startTime = new Date('2023-01-19T00:00:00+02:00');

ZoomMtg.init({
    apiKey: apiKey,
    apiSecret: apiSecret,
    meetingNumber: meetingId,
    userName: 'Stanley Anyas...',
    passWord: '1000',
    leaveUrl: 'javascript:void(0)',
    role: 0
});

ZoomMtg.join({
    meetingNumber: meetingId,
    userName: 'Stanley Anyas...',
    signature: ZoomMtg.generateSignature({
        meetingNumber: meetingId,
        apiKey: apiKey,
        apiSecret: apiSecret,
        role: 0,
        success: function(res){
            console.log(res.result);
        }
    }),
    success: function(res){
        console.log('Join meeting success');
    },
    error: function(res) {
        console.log(res);
    }
});
